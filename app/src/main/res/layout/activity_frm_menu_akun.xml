<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Frm_menu">

    <TableLayout
        android:id="@+id/tableLayout1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="50dp"
        android:shrinkColumns="*"
        android:stretchColumns="*">

        <TableRow
            android:id="@+id/tableRowBtn"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:layout_marginTop="-1dp"
            android:paddingBottom="10dp">


            <TextView
                android:id="@+id/txa"
                android:padding="2dp"
                android:text=""></TextView>

            <Button
                android:id="@+id/btn_akun"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:drawableTop="@drawable/gb_user"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:textColor="#01579b"
                android:text="Akun Saya"
                android:textStyle="bold"
                android:visibility="visible" />

            <TextView
                android:id="@+id/txb"
                android:padding="2dp"
                android:text=""></TextView>

            <Button
                android:id="@+id/btn_rusak"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:drawableTop="@drawable/gb_kerusakan"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:textColor="#01579b"
                android:text="Kerusakan"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txc"
                android:padding="2dp"
                android:text=""></TextView>
        </TableRow>

        <TableRow
            android:id="@+id/tableRowBtn3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:paddingBottom="10dp">

            <TextView
                android:id="@+id/txa11"
                android:padding="2dp"
                android:text=""></TextView>

            <Button
                android:id="@+id/btn_lokasi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:drawableTop="@drawable/gb_peta"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:textColor="#01579b"
                android:text="Lokasi"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txb13"
                android:padding="2dp"
                android:text=""></TextView>

            <Button
                android:id="@+id/btn_logout"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:drawableTop="@drawable/gb_exit"
                android:gravity="center|center_horizontal"
                android:padding="1dp"
                android:textColor="#01579b"
                android:text="Keluar"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txc3"
                android:padding="2dp"
                android:text=""></TextView>
        </TableRow>

        <TableRow
            android:id="@+id/tableRowBtn2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal">

            <TextView
                android:id="@+id/txi"
                android:padding="2dp"
                android:text=""></TextView>
        </TableRow>

    </TableLayout>

    <TextView
        android:id="@+id/txt_user"
        android:textColor="#FFFFFF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tableLayout1"
        android:text="DINAS PEKERJAAN UMUM"
        android:textColor="#01579b"
        android:textAlignment="center"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView4"
        android:text="KOTA BANDAR LAMPUNG"
        android:textColor="#01579b"
        android:textAlignment="center"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/txt_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="69dp"
        android:textColor="#FFFFFF"
        android:text="TextView" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="286dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/imageView2"
        android:text="Jl. Pulau Sebesi No. 68 , Sukarame kota Bandar Lampung 35131
No. Telpon (0721) 7620300"
        android:textAlignment="viewEnd"
        android:textColor="#01579b" />


</RelativeLayout>